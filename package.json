{
  "name": "pocket-pbt",
  "private": true,
  "version": "1.2.0",
  "author": "keyobs",
  "type": "module",
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test --filter=...[HEAD^1]",
    "test:run": "turbo run test:run --filter=...[HEAD^1]",
    "format": "prettier -w .",
    "prepare": "husky"
  },
  "devDependencies": {
    "turbo": "^2.5.8",
    "typescript": "~5.6.2",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "eslint": "^9.13.0",
    "@eslint/js": "^9.13.0",
    "typescript-eslint": "^8.11.0",
    "globals": "^15.11.0",
    "vite": "^5.4.14",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "@types/react": "^19.2.2"
  }
}
